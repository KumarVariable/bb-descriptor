{
  "key": "com.kunal.bitbucket.plugins.codefixagent",
  "name": "code fixer agent",
  "description": "get your code fixed reviewed with artificially intelligent tool",
  "vendor": {
    "name": "KUNAL KMR",
    "url": "https://bito.ai/"
  },
  "baseUrl": "https://90f661bb1893.ngrok-free.app/int/api/v1",
  "authentication": {
    "type": "jwt"
  },
  "lifecycle": {
    "installed": "/installed",
    "uninstalled": "/uninstalled"
  },
  "modules": {
    "webhooks": [
      {
        "event": "pullrequest:created",
        "url": "/webhook/bitbucket"
      },
      {
        "event": "pullrequest:updated",
        "url": "/webhook/bitbucket"
      },
      {
        "event": "pullrequest:fulfilled",
        "url": "/webhook/bitbucket"
      },
      {
        "event": "pullrequest:rejected",
        "url": "/webhook/bitbucket"
      },
      {
        "event": "pullrequest:comment_created",
        "url": "/webhook/bitbucket"
      },
      {
        "event": "pullrequest:comment_updated",
        "url": "/webhook/bitbucket"
      }
    ]
  },
  "scopes": ["account", "repository", "pullrequest"],
  "contexts": ["account", "repository"]
}
